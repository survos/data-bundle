{
    "name": "survos/data-bundle",
    "description": "Shared data directory conventions and path utilities for dataset-driven apps (APP_DATA_DIR).",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
        "Survos\\DataBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        "Survos\\DataBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "doctrine/dbal": "^3.9 || ^4.2",
        "symfony/framework-bundle": "^7.4 || ^8.0",
        "symfony/dependency-injection": "^7.4 || ^8.0",
        "symfony/config": "^7.4 || ^8.0",
        "symfony/filesystem": "^7.4 || ^8.0",
        "symfony/console": "^7.4 || ^8.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^12.0",
        "symfony/yaml": "^7.4 || ^8.0",
        "survos/import-bundle": "^2.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
